{
  "name": "@ndjson-utils/vega-lite",
  "version": "0.0.2",
  "description": "Create vega-lite spec from json/ndjson",
  "keywords": [
    "ndjson",
    "vega-lite",
    "cli"
  ],
  "author": "idris-maps",
  "license": "GPLv2",
  "bin": {
    "bar": "dist/bar.js"
  },
  "directories": {
