  "repository": {
    "type": "git",
    "url": "https://github.com/idris-maps/ndjson-utils.git"
  },
  "homepage": "https://github.com/idris-maps/ndjson-utils/tree/master/packages/sqlite",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
